      {{section.settings.custom_html_1}}
      {%- endif -%}
      {%-  if section.settings.custom_html_2_position == 'right' -%}
      {{section.settings.custom_html_2}}
      {%- endif -%}
      {%- endif -%}
    </div>
  </div>
