      {%-  if section.settings.custom_html_1_position == 'left' -%}
          {{section.settings.custom_html_1}}
      {%- endif -%}
      {%-  if section.settings.custom_html_2_position == 'left' -%}
          {{section.settings.custom_html_2}}
      {%- endif -%}
      {%- endif -%}
    </div>
  
    <div class="slider-content-middle{%- if section.settings.slider_width == "0" -%}{{" hide"}}{%- else -%}{{" "}}{{section.settings.slider_width}}{%- endif -%}{%- if section.settings.visible_on_mobile == false -%}{{" hidden-xs"}}{%- endif-%}">
      {%- if showTop-%}
          {%-  if section.settings.custom_html_1_position == 'center_top' -%}
              {{section.settings.custom_html_1}}
          {%- endif -%}
          {%-  if section.settings.custom_html_2_position == 'center_top' -%}
              {{section.settings.custom_html_2}}
          {%- endif -%}
      {%- endif -%}
      <div class="slider-main-content{%- if section.settings.visible_on_mobile == false -%}{{" hidden-xs"}}{%- endif-%}">
      {%- include 'slider-content' -%}
      </div>
      {%- if showBottom -%}
          {%-  if section.settings.custom_html_1_position == 'center_bottom' -%}
              {{section.settings.custom_html_1}}
          {%- endif -%}
          {%-  if section.settings.custom_html_2_position == 'center_bottom' -%}
              {{section.settings.custom_html_2}}
          {%- endif -%}
      {%- endif -%}
    </div>  

    <div class="slider-content-right{%- if section.settings.custom_html_width_right == "0" -%}{{" hide"}}{%- else -%}{{" "}}{{section.settings.custom_html_width_right}}{%- endif -%}{%- if section.settings.html_visible_on_mobile_right == false -%}{{" hidden-xs"}}{%- endif-%}">
      {%- if showRight -%}
