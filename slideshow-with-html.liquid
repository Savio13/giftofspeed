<style>
  .slider-section-wrapper {
  	position: relative;
    padding-top: 30px;
    overflow: hidden;
  }
  /* slider placement */ 
  {%- if section.settings.slider_placement == 'full-width' -%}
  #shopify-section-{{section.id}} .slider-wrapper {
  	padding-left: 0;
    padding-right: 0;
  }
  #shopify-section-{{section.id}} .slider-content-middle {
  	padding-left: 0;
    padding-right: 0;
  }
  {%- endif -%}
  /* @start Home Slider */
  #home-slider-{{section.id}} {
  	{% if section.settings.slider_margintop != '' %}
    margin-top:{{section.settings.slider_margintop}}px;
    {% endif %}
    overflow: hidden;
  }
  #home-slider-{{section.id}} .slide-body{
    height: {{section.settings.slider_height | default:500 | plus:0}}px;
    {% if section.settings.slider_auto_height %}
    height: auto!important;
    {% endif %}
  }
  #home-slider-{{section.id}} .slide-banner{
    z-index: 1;
    position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background-size: cover;
    background-repeat: no-repeat;
    background-position: 50% 50%;
    {% if section.settings.slider_auto_height %}
    position:relative!important;
    {% endif %}
  }
  
  #home-slider-{{section.id}} .slide-caption.caption.header {
    -ms-box-orient: horizontal;
    display: -webkit-box;
    display: -moz-box;
    display: -ms-flexbox;
    display: -moz-flex;
    display: -webkit-flex;
    display: flex;
    min-width: 100%!important;
    min-height: 100%!important;
  }
  
  #home-slider-{{section.id}} .slide-caption.caption.header.vert-align-top {
    align-items: flex-start;
  }
  #home-slider-{{section.id}} .slide-caption.caption.header.vert-align-middle {
    align-items: center;
  }
  #home-slider-{{section.id}} .slide-caption.caption.header.vert-align-bottom {
    align-items: flex-end;
  }
  #home-slider-{{section.id}} .slide-caption.caption.header.hori-align-left {
    justify-content: flex-start;
  }
  #home-slider-{{section.id}} .slide-caption.caption.header.hori-align-center {
    justify-content: center;
  }
  #home-slider-{{section.id}} .slide-caption.caption.header.hori-align-right {
    justify-content: flex-end;
  }
  
  #home-slider-{{section.id}} .slide-img{
    visibility: hidden;
  }
  #home-slider-{{section.id}} .slide-video{
    position: absolute;
    top: 0;
    left: 0;
    bottom: 0;
    right: 0;
    min-width: 100%;
    min-height: 100%;
    z-index: 1;
  }
  #home-slider-{{section.id}} .slide-caption-wrapper {
    display:inline-block; 
    padding: 30px 30px 5px;
  }
  #home-slider-{{section.id}} .slide-sticker-wrapper {
    display:inline-block; 
  }
  #home-slider-{{section.id}} .caption.sub{
    display: inline-block;
  }
  #home-slider-{{section.id}} .caption.header h2{
    font-weight: 300;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
  }
  #home-slider-{{section.id}} .caption-content.real-caption {
    font-weight: 300;
    -webkit-font-smoothing: antialiased;
    -moz-osx-font-smoothing: grayscale;
  }
  {%- for block in section.blocks limit: section.blocks.size -%} 
  #home-slider-{{section.id}} ul.slides #slide-item-{{block.id}} h2.caption-content {
    {%- if block.settings.slide_head_decor == '0' -%}
    margin-bottom:10px;
    {%- elsif block.settings.slide_head_decor == '1' -%}
    border:1px solid #ffffff;
    padding:10px;
    margin-bottom:10px;
    {%- else -%}
    border-bottom:1px solid #ffffff;
    padding-bottom:2px;
    margin-bottom:7px;
    {%- endif -%}

    {%- if block.settings.slide_head_space -%}
    letter-spacing:0.09em;
    {%- endif -%}

    {%- if block.settings.slide_head_bodyfont -%}
    font-family:sans-serif;
    {%- endif -%}
  }
    

  #home-slider-{{section.id}} ul.slides #slide-item-{{block.id}} .real-caption {
    {%- if block.settings.slide_capt_decor == '0' -%}
    margin-bottom:10px;
    {%- elsif block.settings.slide_capt_decor == '1' -%}
    border:1px solid #ffffff;
    padding:10px;
    margin-bottom:10px;
    {%- else -%}
    border-bottom:1px solid #ffffff;
    padding-bottom:2px;
    margin-bottom:7px;
    {%- endif -%}
  }

  #home-slider-{{section.id}} ul.slides #slide-item-{{block.id}} .caption-link {
    {%- if block.settings.slide_cta_undrline -%}
    border-bottom:1px solid #ffffff;
    padding-bottom:10px;
    margin-bottom:7px;
    {%- else -%}
    margin-bottom:10px;
    {%- endif -%}

    {%- if block.settings.slide_cta_btw == false -%}
    margin-top:19px;
    {%- endif- %} 
  }
  {%- endfor -%}
  {%- if section.settings.slider_mask -%}
  .flexslider .slides .slider-mask {
    position: absolute;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background-color:{{section.settings.slider_mask_color}};
    opacity:{{section.settings.slider_mask_opacity}};
  }
  {%- endif -%}
  #home-slider-overlay span {
    background-image: url({{'loader.gif' | asset_url}});
  }
  {%- if settings.advanced_prev_next == '2' -%}
  .flex-direction-nav a {
    -webkit-border-radius: 0px!important;
    -moz-border-radius: 0px!important;
    border-radius: 0px!important;
  }
  {%- endif -%}
  #home-slider-{{section.id}} .flex-direction-nav a {
    border-color: {{section.settings.slider_nav_border_color | default: 'transparent'}};
    background-color: {{section.settings.slider_nav_bg_color | default: 'transparent'}};
  }
  #home-slider-{{section.id}} .flex-direction-nav a svg {
    fill:{{section.settings.slider_nav_icon_color}};
    will-change: fill;
    transition: 200ms linear;
  }
  #home-slider-{{section.id}} .flex-direction-nav li:hover a {
    border-color: {{section.settings.slider_navhvr_border_color | default: 'transparent'}};
    background-color: {{section.settings.slider_navhvr_bg_color | default: 'transparent'}};
  }
  #home-slider-{{section.id}} .flex-direction-nav li:hover a svg {fill:{{section.settings.slider_navhvr_icon_color}};}
  /* @end Home Slider */
</style>
