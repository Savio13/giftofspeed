<div class="{%- if section.settings.slider_placement == 'within-container' -%}{{" container"}}{%- endif -%}{{" col-sm-12 slider-wrapper"}}" data-section-id="{{ section.id }}" data-section-type="slideshow-with-html">
  <div class="row">
    <div class="slider-content-left{%- if section.settings.custom_html_width_left == "0" -%}{{" hide"}}{%- else -%}{{" "}}{{section.settings.custom_html_width_left}}{%- endif -%}{%- if section.settings.html_visible_on_mobile_left == false -%}{{" hidden-xs"}}{%- endif-%}">
